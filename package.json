{
  "name": "inventory1",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^7.3.1",
    "@mui/lab": "^7.0.0-beta.16",
    "@mui/material": "^7.3.1",
    "@mui/x-date-pickers": "^8.10.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/pg": "^8.15.5",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "framer-motion": "^12.23.12",
    "lottie-react": "^2.4.1",
    "pg": "^8.16.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.1",
    "react-spring": "^10.0.1",
    "react-transition-group": "^4.4.5",
    "recharts": "^3.1.2",
    "styled-components": "^6.1.19",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "vite",
    "test": "vitest",
    "setup": "node setup-check.js",
    "setup:auto": "call SETUP.bat",
    "setup:docker": "call DOCKER_SETUP.bat",
    "backend": "cd backend && ../.venv/Scripts/python.exe main.py",
    "backend:main": "cd backend && ../.venv/Scripts/python.exe main.py",
    "backend:test": "cd backend && ../.venv/Scripts/python.exe test_server.py",
    "full-dev": "concurrently \"npm run dev\" \"npm run backend\"",
    "server": "npm run backend",
    "api": "cd backend && ../.venv/Scripts/python.exe -m uvicorn inventory_api:app --reload --host 0.0.0.0 --port 8000",
    "start:system": "call scripts/START.bat",
    "stop:system": "call scripts/STOP.bat",
    "setup:database": "call scripts/SETUP_DATABASE.bat",
    "update:deps": "call scripts/UPDATE_DEPS.bat",
    "test:ocr": "cd backend && ../.venv/Scripts/python.exe ocr_setup.py",
    "docker:start": "docker-compose up --build -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:status": "docker-compose ps",
    "docker:clean": "docker-compose down -v --remove-orphans",
    "docker:rebuild": "docker-compose down && docker-compose build --no-cache && docker-compose up -d",
    "one-click": "call SIMPLE-DOCKER-START.bat",
    "one-click-start": "call SIMPLE-DOCKER-START.bat",
    "one-click-stop": "call ONE-CLICK-STOP.bat",
    "docker-status": "call DOCKER-STATUS.bat",
    "docker-troubleshoot": "call DOCKER-TROUBLESHOOT.bat",
    "docker-fix": "call ENABLE-DOCKER.bat",
    "enable-docker": "call ENABLE-DOCKER.bat",
    "local": "call LOCAL-START.bat",
    "local-start": "call LOCAL-START.bat",
    "local-stop": "call LOCAL-STOP.bat",
    "local-status": "call LOCAL-STATUS.bat",
    "quick": "call LOCAL-START.bat",
    "start-local": "call LOCAL-START.bat",
    "wsl": "call WSL-START.bat",
    "wsl-start": "call WSL-START.bat",
    "wsl-status": "call WSL-STATUS.bat"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "vite": "^5.4.8",
    "vitest": "^3.2.4"
  }
}
